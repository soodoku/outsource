citHeader("To cite package 'outsource' in publications use:")
     
year <- sub(".*(2[[:digit:]]{3})-.*", "\\1", meta$Date, perl = TRUE)
vers <- paste("R package version", meta$Version)
 
citEntry(entry="Manual",
		  title = "Outsource: Tool for Creating Distributed Coding Tasks",
		  author = personList(as.person("Gaurav Sood")),
		  year = year,
		  note = vers,
		  textVersion =
		  paste("Gaurav Sood (",
				year,
				"). Outsource: Tool for Creating Distributed Coding Tasks. ",
				vers, ".", sep=""))